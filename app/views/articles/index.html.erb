<h1>Wikicomplex#index</h1>

<%= link_to "[create]", new_article_path(@article)%>

<ul>
  <% @articles.each do |article| %>

    <li>
      <h2>
        <%= link_to article.title, article_path(article)%>
        <%= link_to "[Delete]", article_path(article),
        method: :delete,
        data: { confirm: "Are you sure?" } %> <%= link_to "[update]", edit_article_path(article)%>
      </h2>
      <p><%=article.content %> </p>
    </li>
  <% end %>

</ul>



